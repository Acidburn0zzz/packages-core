pkgname=manjaro-release
pkgver=0.8.8
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('f775420213e5bb88c483bba3bef5fe7a4a583e41e331256ede2a00af118edadb')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
