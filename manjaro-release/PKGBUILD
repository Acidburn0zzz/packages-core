pkgname=manjaro-release
pkgver=0.8.6
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('caa2c2329d9c9a61c66b9c7a0d86786ac6f9b7be1467ee09f030e2ce2d2cc7a5')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
